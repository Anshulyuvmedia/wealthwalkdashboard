{
  "name": "TdUser",
  "base": "User",
  "idInjection": true,
  "properties": {
    "contactName": {
      "type": "string",
      "required": true
    },
    "userType": {
      "type": "string",
      "required": true,
      "default": "user"
    },
    "city": {
      "type": "string"
    },
    "state": {
      "type": "string"
    },
    "country": {
      "type": "string",
      "default": "IN"
    },
    "referrald": {
      "type": "string"
    },
    "otp": {
      "type": "string"
    },
    "codeCreatedAt": {
      "type": "date"
    },
    "ipAddress": {
      "type": "string"
    },
    "twoFaEnabled": {
      "type": "boolean",
      "default": false
    },
    "profileImage": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "default": "pending"
    },
    "phone": {
      "type": "string"
    },
    "phoneVerified": {
      "type": "boolean",
      "default": false
    },
    "isTermsAgreed": {
      "type": "boolean",
      "default": false
    },
    "lastLogin": {
      "type": "date"
    },
    "tenantCode": {
      "type": "string",
      "required": true,
      "default": "ADB"
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "AccessToken",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW"
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "user",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "user",
      "permission": "ALLOW",
      "property": "updateAttributes"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW",
      "property": [
        "generateOtp",
        "sendOTP",
        "verifyOTP",
        "loginGenerateOtp",
        "loginVerifyOtp",
        "registerWithPassword",
        "loginWithPassword"
      ]
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW",
      "property": [
        "resetUserPassFromPanel",
        "changeUserStatus",
        "resetUserPassword",
        "generateAccessToken",
        "testSMS",
        "logout",
        "upload"
      ]
    },
    {
      "accessType": "READ",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "ALLOW"
    },
    {
      "accessType": "WRITE",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}